{
  "manifest_version": 2,
  "name": "The duplicator",
  "description": "Duplicate extension with an extra url option",
  "version": "0.2",
  "browser_action": {
  	"default_icon": "copy.png",
    "default_popup": "popup.html",
    "default_title": ""
  },
  "icons": {
      "128": "copy.png",
      "16": "copy.png",
      "48": "copy.png"
   },
  "permissions": [
  	"activeTab",
    "storage",
    "tabs"
  ]
}